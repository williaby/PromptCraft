{
  "prometheus": {
    "enabled": false,
    "pushgateway_url": "http://localhost:9091",
    "job_name": "token_optimization",
    "instance": "default",
    "scrape_interval": "30s",
    "metrics_prefix": "token_optimization"
  },
  "grafana": {
    "enabled": false,
    "api_url": "http://localhost:3000/api",
    "api_key": null,
    "org_id": 1,
    "dashboard_id": "token-optimization",
    "auto_create_dashboard": true,
    "dashboard_tags": ["token-optimization", "performance", "monitoring"]
  },
  "datadog": {
    "enabled": false,
    "api_key": null,
    "app_key": null,
    "site": "datadoghq.com",
    "service_name": "token-optimization",
    "env": "production",
    "tags": ["service:token-optimization", "team:platform"]
  },
  "cloudwatch": {
    "enabled": false,
    "region": "us-east-1",
    "namespace": "TokenOptimization",
    "aws_access_key": null,
    "aws_secret_key": null,
    "metric_filters": {
      "include_patterns": ["token_reduction_*", "loading_latency_*", "success_rate_*"],
      "exclude_patterns": ["debug_*", "test_*"]
    }
  },
  "slack": {
    "enabled": false,
    "webhook_url": null,
    "channel": "#alerts",
    "username": "Token Optimization Monitor",
    "icon_emoji": ":chart_with_upwards_trend:",
    "alert_levels": ["error", "warning"],
    "rate_limit_minutes": 5
  },
  "pagerduty": {
    "enabled": false,
    "integration_key": null,
    "service_key": null,
    "severity_mapping": {
      "critical": "critical",
      "error": "error", 
      "warning": "warning",
      "info": "info"
    }
  },
  "webhook": {
    "enabled": false,
    "url": null,
    "method": "POST",
    "headers": {
      "Content-Type": "application/json",
      "Authorization": "Bearer YOUR_TOKEN"
    },
    "timeout_seconds": 30,
    "retry_attempts": 3
  },
  "email": {
    "enabled": false,
    "smtp_server": "smtp.gmail.com",
    "smtp_port": 587,
    "username": null,
    "password": null,
    "from_address": "token-optimization@yourcompany.com",
    "to_addresses": ["alerts@yourcompany.com"],
    "subject_prefix": "[Token Optimization]"
  }
}